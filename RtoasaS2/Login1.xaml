<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RtoasaS2.Login1">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Ventana Login" FontSize="Medium"
                />

            <Entry x:Name="txtUsuario" Placeholder="Ingrese el usuario"/>
            <Entry x:Name="txtContrasena" Placeholder="Ingrese la contrasena"/>
            <Button x:Name="btnIniciar" Text="INICIAR" Clicked="btnIniciar_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>